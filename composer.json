{
    "name": "mrgoodbytes8667/cached-http-client-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for easily caching HttpClient responses",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "symfony/cache": "^7.2",
        "symfony/config": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/filesystem": "^7.2",
        "symfony/http-client": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/string": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.58",
        "mrgoodbytes8667/test-common-faker": "^0.6",
        "rector/rector": "^2.0",
        "symfony/clock": "^7.2",
        "symfony/test-pack": "^1.0.8"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "conflict": {
        "phpunit/phpunit": "<9.5 | 9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\HttpClient\\Cached\\": "src/",
            "Bytes\\HttpClient\\Cached\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
